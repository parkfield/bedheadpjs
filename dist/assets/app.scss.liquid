.filter-menu-container .filter-value {
  @include flex();
  margin-right: 25px;
  .wrapper .checkbox{
    margin-right: 15px;
    width: 25px;
    height: 25px;
    position: relative;
  }
  .wrapper .checkbox:before {
    display: block;
    content:'';
    cursor: pointer;
    position: absolute;
    width: 25px;
    height: 25px;
    top: 0;
    left: 0;
    background: transparent;
    border:thin solid #ddd;
  }
  .wrapper .checkbox:after {
    opacity: 0;
    content: '';
    position: absolute;
    width: 9px;
    height: 5px;
    background: transparent;
    top: 35%;
    left: 35%;
    @include prefix(transform, translate(-50%, -50%));
    border: 3px solid #333;
    border-top: none;
    border-right: none;
    transform: rotate(-45deg);
    &:hover{
      cursor: pointer;
    }
  }
  .wrapper input{
    position: absolute;
    width: 25px;
    height: 25px;
    z-index: 2;
    opacity: 0;
    &:hover{
      cursor: pointer;
    }
  }
  /**
   * Create the checkbox state for the tick
   */
  .wrapper input[type=checkbox]:checked + .checkbox:after {
    opacity: 1;
    border-color: white;
  }
  .wrapper input[type=checkbox]:checked + .checkbox:before {
    background-color: black;
  }

  label{
    @include lato-reg(.75rem);
    text-transform: uppercase;
  }
}


