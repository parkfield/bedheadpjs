<!--[if IE 9]><html class="ie9 no-js" lang="{{ shop.locale }}"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--><html class="no-js" lang="{{ shop.locale }}"><!--<![endif]-->

<head>
  <!-- META ================================================================ -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  <meta name="theme-color" content="{{ settings.color_primary }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <title>{{ shop.name }} &ndash; {{ 'password_page.opening_soon' | t }}</title>
  {% include 'meta-social-tags' %}
  {% include 'meta-favorites' %}
  <!-- Shopify ============================================================= -->
  {{ content_for_header }}
  <!-- CSS ================================================================= -->
  {{ 'app.scss.css' | asset_url | stylesheet_tag }}
  <!-- JavaScript =========================================================== -->
  {% if shop.customer_accounts_enabled and template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
  {% endif %}
  {{ 'vendor.js' | asset_url | script_tag }}
</head>

<body class="template-password" itemscope itemtype="http://schema.org/WebSite">

  <main id="MainContent" role="main">
    {{ content_for_layout }}
  </main>

</body>
</html>
